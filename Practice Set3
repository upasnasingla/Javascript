    <script>
        //Q1:print marks of student in an object using for loop
        let obj={
            harry:98,
            Rohan:65,
            aakash:7
        }
        for(let i=0;i<Object.keys(obj).length;i++){      
            console.log("The marks of "+Object.keys(obj)[i]+" are "+obj[Object.keys(obj)[i]]);
        }

        //Q2:Above question using for in loop
        for(let x in obj){
            console.log("The marks of "+ x+" are "+obj[x]);
        }


        //Q3:Print try again until user enters correct no
        let cn=15;
        let i;
        let k;
        while(cn!=k){
            i=prompt("Enter a no");
             k=parseInt(i);
            console.log("Try Again");
                }
        console.log("Entered correct no");
       


        //Q4:Find mean of numbers using functions
        const mean=(a,b,c,d)=>{
            return (a+b+c+d)/4;
        }
        console.log(mean(12,14,15,14));
    
        
    </script>
    
